{
  "name": "lotlogic-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "upload": "aws s3 sync ./dist s3://dev.blockplanner.com.au --delete",
    "clear": "aws cloudfront create-invalidation --distribution-id EPAVQRHYCIFEE --paths '/*' ",
    "deploy": "yarn build && yarn upload && yarn clear",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@tailwindcss/vite": "^4.1.18",
    "@uidotdev/usehooks": "^2.4.1",
    "@vis.gl/react-google-maps": "^1.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.536.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.71.1",
    "react-router-dom": "^7.12.0",
    "sass": "^1.97.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.3.6",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "husky": "^9.1.7",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  }
}
